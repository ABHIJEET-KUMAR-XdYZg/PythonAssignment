   "source": [
    "# Write a Python program to check whether a character is uppercase or lowercase alphabet. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "5188ae16",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the character : A\n",
      "Uppercase\n"
     ]
    }
   ],
   "source": [
    "c=input(\"Enter the character : \")\n",
    "if(c>='A' and c<='Z'):\n",
    "    print(\"Uppercase\")\n",
    "elif(c>='a' and c<='z'):\n",
    "    print(\"Lowercase\")\n",
    "else:\n",
    "    print(\"Not an alphabet\")"
   ]
  }
