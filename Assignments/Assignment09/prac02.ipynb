   "source": [
    "# Write a Python program to check whether a number is divisible by 5 and 11 or not. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "0a048ecf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the number : 55\n",
      "Entered number is not divisible\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter the number : \"))\n",
    "if(n%5 and n%11):\n",
    "    print(\"Entered number is divisible by 5 and 11\")\n",
    "else:\n",
    "    print(\"Entered number is not divisible\")"
   ]
