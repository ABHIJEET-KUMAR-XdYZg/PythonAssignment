   "source": [
    "# Write a Python program to input any character and check whether it is alphabet, digit or special character. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9588ff56",
   "metadata": {},
   "outputs": [],
   "source": [
    "c=input(\"Enter the character : \")\n",
    "if(c.isalpha()):\n",
    "    print(\"Alphabet\")\n",
    "elif(c.isdigit()):\n",
    "    print(\"Digit\")\n",
    "else:\n",
    "    print(\"Special Char\")"
   ]
  }
