   "source": [
    "# Write a Python Program to remove punctuation from a given string (using loop) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "dab93a6e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the string : abc!...def!=-ghi?>thy-\n",
      "abcdef=ghi>thy\n"
     ]
    }
   ],
   "source": [
    "def operation(s):\n",
    "    l=[',',\"'\",'-','...','.','!','?']\n",
    "    for c in l:\n",
    "        s=s.replace(c,'')\n",
    "    return s\n",
    "s=operation(input(\"Enter the string : \"))\n",
    "print(s)"
   ]
  }
