   "source": [
    "def operation(s):\n",
    "    l=['a','e','i','o','u']\n",
    "    for i in range(5):\n",
    "        print(f\"No of occurences of {l[i]} : {s.count(l[i])}\")\n",
    "operation(input(\"Enter the string : \"))"
   ]
