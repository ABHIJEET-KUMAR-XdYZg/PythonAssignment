   ],
   "source": [
    "def factorial(n):\n",
    "    if(n<0):\n",
    "        print(\"Invalid input\")\n",
    "        exit(0)\n",
    "    p=1\n",
    "    for i in range(2,n+1):\n",
    "        p*=i;\n",
    "    return p\n",
    "print(factorial(int(input(\"Enter the number : \"))))"
   ]
  }
